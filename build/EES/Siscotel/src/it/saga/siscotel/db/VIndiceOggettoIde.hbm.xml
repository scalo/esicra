<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="it.saga.siscotel.db.VIndiceOggettoIde"
           table="v_indice_oggetto_ide" schema="esicra" mutable="false">
        <id name="aotPkid" type="big_decimal">
            <column name="aot_pkid" precision="22" scale="0"/>
            <generator class="assigned"/>
        </id>
        <property name="codiceEcografico" type="string">
            <column name="codice_ecografico" length="70"/>
        </property>
        <property name="codAot" type="string">
            <column name="cod_aot" length="20"/>
        </property>
        <property name="codCatasto" type="string">
            <column name="cod_catasto" length="20"/>
        </property>
        <property name="aotDtmod" type="timestamp">
            <column name="aot_dtmod" length="8"/>
        </property>
        <property name="aotCodIstat" type="string">
            <column name="aot_cod_istat" length="20"/>
        </property>
        <property name="tipoCatasto" type="string">
            <column name="tipo_catasto" length="20"/>
        </property>
        <property name="desIdentificativo" type="string">
            <column name="des_identificativo" length="20"/>
        </property>
        <property name="idComune" type="string">
            <column name="id_comune" length="20"/>
        </property>
        <property name="sezione" type="string">
            <column name="sezione" length="5"/>
        </property>
        <property name="foglio" type="string">
            <column name="foglio" length="5"/>
        </property>
        <property name="mappale" type="string">
            <column name="mappale" length="20"/>
        </property>
        <property name="sub" type="string">
            <column name="sub" length="5"/>
        </property>
        <property name="protAnno" type="integer">
            <column name="prot_anno"/>
        </property>
        <property name="protNum" type="string">
            <column name="prot_num" length="20"/>
        </property>
        <property name="idEnte" type="integer">
            <column name="id_ente" precision="7" scale="0"/>
        </property>
        <property name="ideDtMod" type="timestamp">
            <column name="ide_dt_mod" length="8"/>
        </property>
    </class>
</hibernate-mapping>
