<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="it.saga.siscotel.db.ProTipo" table="pro_tipo" schema="esicra">
        <id name="pkid" type="big_decimal" unsaved-value="null"  >
        <column name="PKID" sql-type="numeric(22,0)" not-null="true"/>
          <generator class="it.saga.siscotel.db.hibernate.PkidGenerator">
        </generator>
        </id>
        <property name="descrizione" type="string">
            <column name="descrizione" length="50" />
        </property>
    </class>
</hibernate-mapping>
