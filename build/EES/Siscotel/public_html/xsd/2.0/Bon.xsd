<xs:schema targetNamespace="http://www.saga.it/egov/esicra/importazione/xml/esicra_import" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:esicra_import="http://www.saga.it/egov/esicra/importazione/xml/esicra_import" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:complexType name="BonType">
		<xs:sequence>
			<xs:element name="CodBolletta">
				<xs:annotation>
					<xs:documentation>Codice univoco bolletta        tra servizi e anni</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="IdServizio" type="xs:long"/>
			<xs:element name="CodServizio">
				<xs:annotation>
					<xs:documentation>RSU, ICI,POL,TOS,....</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Bon" type="esicra_import:BonType"/>
</xs:schema>
