<?xml-stylesheet type="text/xsl" href="out_struttura.xsl"?>
<xs:schema targetNamespace="http://www.saga.it/egov/esicra/esportazione/xml/esicra_pra" xmlns:esicra_pra="http://www.saga.it/egov/esicra/esportazione/xml/esicra_pra" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="Comune.xsd"/>
	<xs:include schemaLocation="Atto.xsd"/>
	<xs:include schemaLocation="Localita.xsd"/>
	<xs:complexType name="StatoCivileType">
		<xs:sequence>
			<xs:element name="CodStatoCivile" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DesStatoCivile">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="80"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DataStatoCivile" type="xs:date"/>
			<xs:element name="CodConiuge">
				<xs:annotation>
					<xs:documentation>Codice Fiscale Coniuge</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CognomeConiuge">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NomeConiuge">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:choice>
				<xs:element name="Comune" type="esicra_pra:ComuneType"/>
				<xs:element name="Localita" type="esicra_pra:LocalitaType"/>
			</xs:choice>
			<xs:element name="Atto" type="esicra_pra:Atto"/>
			<xs:element name="DataInizio" type="xs:date"/>
			<xs:element name="DataFine" type="xs:date" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
