				</xs:simpleType>
			</xs:element>
			<xs:element name="AnnoDenuncia" type="xs:integer"/>
			<xs:element name="NumDenuncia">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DataDenuncia" type="xs:date" default="2004-01-01"/>
